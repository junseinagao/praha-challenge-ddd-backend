{
  "name": "praha-challenge-ddd-backend-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "STAGE=production node dist/src/main",
    "dev": "dotenv -e .local.env -- nest start --watch",
    "dev:debug": "dotenv -e .local.env -- nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "pnpm lint --fix",
    "migrate:dev": "dotenv -e .local.env -- prisma migrate dev --preview-feature",
    "model-generate": "prisma generate",
    "migrate:dev:reset": "dotenv -e .local.env -- prisma migrate reset --preview-feature",
    "migrate:test": "dotenv -e .test.env -- prisma migrate reset --force --preview-feature",
    "migrate:prd": "prisma migrate deploy --preview-feature",
    "test:unit": "dotenv -e .test.env -- jest",
    "test:integration": "pnpm migrate:test && dotenv -e .test.env -- jest -c ./jest.integration.config.js --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.3",
    "@nestjs/core": "^10.0.3",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/swagger": "^7.0.3",
    "@prisma/client": "4.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv": "^6.3.1",
    "dotenv": "^16.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "4.16.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "volta": {
    "node": "18.15.0"
  }
}